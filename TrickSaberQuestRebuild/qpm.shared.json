{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.shared.schema.json",
  "config": {
    "version": "0.0.1",
    "sharedDir": "shared",
    "dependenciesDir": "extern",
    "info": {
      "name": "TrickSaber",
      "id": "tricksaber",
      "version": "0.0.1",
      "url": "https://github.com/SeasonedMiso/TrickSaberQuest",
      "additionalData": {
        "overrideSoName": "libtricksaber.so",
        "cmake": true
      }
    },
    "workspace": {
      "scripts": {
        "build": [
          "pwsh ./scripts/build.ps1"
        ],
        "clean": [
          "pwsh ./scripts/build.ps1 -clean"
        ],
        "copy": [
          "pwsh ./scripts/copy.ps1 $0:?"
        ],
        "logcat": [
          "pwsh ./scripts/start-logging.ps1"
        ],
        "qmod": [
          "pwsh ./scripts/build.ps1",
          "pwsh ./scripts/createqmod.ps1"
        ],
        "restart": [
          "pwsh ./scripts/restart-game.ps1"
        ],
        "stack": [
          "pwsh ./scripts/ndk-stack.ps1"
        ],
        "tomb": [
          "pwsh ./scripts/pull-tombstone.ps1"
        ]
      },
      "qmodIncludeDirs": [
        "./build",
        "./extern/libs"
      ],
      "qmodIncludeFiles": [],
      "qmodOutput": "./binaries/TrickSaber.qmod"
    },
    "dependencies": [
      {
        "id": "beatsaber-hook",
        "versionRange": "^5.1.9",
        "additionalData": {}
      },
      {
        "id": "scotland2",
        "versionRange": "^0.1.6",
        "additionalData": {
          "includeQmod": false
        }
      },
      {
        "id": "paper2_scotland2",
